-- â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
local ESP_Highlights = {}
local function removeHighlight(obj)
    if ESP_Highlights[obj] then
        ESP_Highlights[obj]:Destroy()
        ESP_Highlights[obj] = nil
    end
end
local function addHighlight(obj, fillColor)
    removeHighlight(obj)
    local hl = Instance.new("Highlight")
    hl.Adornee = obj
    hl.FillColor = fillColor
    hl.OutlineColor = fillColor
    hl.FillTransparency = 0.5
    hl.OutlineTransparency = 0
    hl.Parent = obj
    ESP_Highlights[obj] = hl
end
local function getActiveMap()
    return workspace.Maps:FindFirstChildOfClass("Model") or workspace.Maps:GetChildren()[1]
end
local function updateMapESP(folderName, color, enabled)
    local map = getActiveMap()
    if not map or not map:FindFirstChild(folderName) then return end
    local folder = map[folderName]
    if enabled then
        for _, obj in pairs(folder:GetChildren()) do
            addHighlight(obj, color)
        end
    else
        for _, obj in pairs(folder:GetChildren()) do
            removeHighlight(obj)
        end
    end
end
-- Map yÃ¼klenince auto ESP (enabled olanlar)
workspace.Maps.ChildAdded:Connect(function(map)
    task.wait(2)
    if CruzSettings.ComputersESP then
        updateMapESP("Computers", Color3.fromRGB(0, 255, 255), true)
    end
    if CruzSettings.FreezersESP then
        updateMapESP("FreezePods", Color3.fromRGB(255, 0, 255), true)
    end
    if CruzSettings.VentsESP then
        updateMapESP("Vents", Color3.fromRGB(255, 255, 0), true)
    end
    if CruzSettings.ExitDoorsESP then
        updateMapESP("ExitDoors", Color3.fromRGB(0, 255, 0), true)
    end
end)
-- Players ESP Loop
task.spawn(function()
    while task.wait(1) do
        if CruzSettings.PlayersESP then
            for _, plr in pairs(Players:GetPlayers()) do
                if plr ~= lplr and plr.Character and not ESP_Highlights[plr.Character] then
                    addHighlight(plr.Character, Color3.fromRGB(255, 0, 0))
                end
            end
        else
            for _, plr in pairs(Players:GetPlayers()) do
                if plr.Character then
                    removeHighlight(plr.Character)
                end
            end
        end
    end
end)
Players.PlayerAdded:Connect(function(plr)
    plr.CharacterAdded:Connect(function()
        task.wait(1)
        if CruzSettings.PlayersESP then
            addHighlight(plr.Character, Color3.fromRGB(255, 0, 0))
        end
    end)
end)
