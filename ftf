VisualsSection:NewToggle("Beast Power Extension", "Beast gÃ¼cÃ¼nÃ¼ ekranda gÃ¶sterir", function(state)
    CruzSettings.BeastExtension = state
    local stats = lplr:FindFirstChild("TempPlayerStatsModule")
    if not stats then return end
    if state then
        local sg = Instance.new("ScreenGui")
        sg.Name = "BeastPowerGui"
        sg.Parent = game.CoreGui
        local frame = Instance.new("Frame", sg)
        frame.Size = UDim2.new(0, 200, 0, 50)
        frame.Position = UDim2.new(0, 10, 0, 10)
        frame.BackgroundColor3 = Color3.new(0,0,0)
        frame.BackgroundTransparency = 0.3
        frame.BorderSizePixel = 0
        local label = Instance.new("TextLabel", frame)
        label.Size = UDim2.new(1,0,1,0)
        label.BackgroundTransparency = 1
        label.TextColor3 = Color3.new(1,1,0)
        label.TextScaled = true
        label.Font = Enum.Font.SourceSansBold
        label.Text = "Beast Power: 0"
        task.spawn(function()
            while CruzSettings.BeastExtension and sg.Parent do
                local power = stats:FindFirstChild("PowerLevel") and stats.PowerLevel.Value or 0
                label.Text = "Beast Power: " .. tostring(power)
                task.wait(0.1)
            end
        end)
    else
        local gui = game.CoreGui:FindFirstChild("BeastPowerGui")
        if gui then gui:Destroy() end
    end
end)
